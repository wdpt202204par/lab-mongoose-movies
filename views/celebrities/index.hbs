<h2>
    <h3><a href="/celebrities/new">Create new celebrity</a></h3>
    {{#each data}}
        <h4><a href="/celebrities/{{_id}}">{{name}}</a></h4>

        <form action="/celebrities/{{_id}}/delete" method="post">
            <button type="submit" name="submit">Delete</button>
        </form>

        <form action="/celebrities/{{_id}}/edit" method="get">
            <button>Edit</button>
        </form>


    {{/each}}
</h2>
